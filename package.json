{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run --prefix client build && npm run build && concurrently \"npm run express-watch\" \"npm run client\"",
    "gen-models": "tsc --lib ES6 templates/*.ts --outDir dist/templates && node dist/templates/index.js",
    "build": "npm run gen-models && tsc -p .",
    "express-watch": "nodemon --ignore src/models/models.type.ts --watch src -e ts,css,html,json,yaml -x \"npm run express\"",
    "express": "npm run build &&  cp src/models/*.yaml dist/models/ && node --inspect dist/index.js",
    "client": "cd client && ./node_modules/.bin/ng serve --watch",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'test/**/*.test.ts'",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/chai-http": "^3.0.3",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.43",
    "@types/mongodb": "^2.2.13",
    "@types/mongoose": "^4.7.23",
    "@types/morgan": "^1.7.33",
    "@types/node": "^8.0.34",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.0",
    "mocha": "^4.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/crypto-js": "^3.1.37",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "dustjs-linkedin": "^2.7.5",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "express-ws": "^3.0.0",
    "faker": "^4.1.0",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.1",
    "mongoose": "^4.12.4",
    "morgan": "^1.9.0",
